{{define "page"}}
    <div class="p-8 w-full">
        <h1 class="pt-4 pb-8 text-3xl font-bold text-gray-800">{{.Title}}</h1>
    </div>
    <div class="columns-4 gap-4 space-y-4 p-8">
        {{range .Images}}
            <div class="h-min w-full">
                <img class="h-full w-full object-cover rounded shadow" src="{{.}}" alt="image">
            </div>
        {{end}}
    </div>
{{end}}
